<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="threedbj.com.viewbuilderexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/sample_title"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.threedbj.viewbuilderexample.SampleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.threedbj.viewbuilderexample.AllWidgetsActivity"
            android:label="@string/all_widgets_title" />
        <activity
            android:name="com.threedbj.viewbuilderexample.ProgrammaticActivity"
            android:label="@string/programmatic_title" />
    </application>

</manifest>